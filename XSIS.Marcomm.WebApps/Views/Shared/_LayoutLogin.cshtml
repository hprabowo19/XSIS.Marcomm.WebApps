<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - XSIS Marcomm System</title>
    @Styles.Render("~/Content/cssvendortemplate")
    @Styles.Render("~/Content/csstemplate")
</head>
<body>
    <div class="login-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquerytemplate")
    @Scripts.Render("~/bundles/bootstraptemplate")
    @Scripts.Render("~/bundles/apptemplate")
    @RenderSection("scripts", required: false)
</body>
</html>
