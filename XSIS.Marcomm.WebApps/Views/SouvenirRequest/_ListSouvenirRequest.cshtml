@model IEnumerable<XSIS.Marcomm.ViewModels.SouvenirStockViewModel>

@{
    int i = 0;
    if (!Model.Any())
    {
        <tr>
            <td colspan="9" class="text-center text-danger">- Data not found -</td>
        </tr>
    }
    foreach (var item in Model)
    {
        <tr>
            <td>
                @(++i)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.requestByToFullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.requestDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.requestDueDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.statusText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.createdDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.createdByToFullName)
            </td>
            <td>
                <a data-toggle="modal" href="#" class="modalMenuAccess" id="Detail" data-id="@item.id" data-status="@item.status">
                    <i class="zmdi zmdi-search zmdi-hc-lg"></i>
                </a>
                <a data-toggle="modal" href="#" class="modalMenuAccess" id="Edit" data-id="@item.id" data-status="@item.status" data-text="@item.statusText" data-access="@item.requestBy">
                    <i class="zmdi zmdi-edit zmdi-hc-lg"></i>
                </a>
            </td>
        </tr>
    }
}